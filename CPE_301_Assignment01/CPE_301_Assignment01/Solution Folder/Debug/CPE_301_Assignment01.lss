                                 start:
                                 
                                 	; *** STORAGE STAGE ***
                                 
                                 	; Initialize Iteration Counter with 299
000097 e25b                      	ldi ITCOUNTERL, (LASTITERATION & 0xFF)
000098 e041                      	ldi ITCOUNTERH, ((LASTITERATION >> 8) & 0xFF)
                                 
                                 	; X will point to the STARTADDS
000099 e0b2                      	ldi XH, high(STARTADDS)
00009a e2a2                      	ldi XL, low(STARTADDS)
                                     ; The Lookup table is stored in ROM, where bytes are stored as words.
                                 	; We need to multiply by 2
00009b e0f0                      	ldi ZH, high(2*DATATABLE)
00009c e0e2                      	ldi ZL, low(2*DATATABLE)
                                 
                                 	; Fill STARTADDS from our Lookup Table
                                     FillLoop:
00009d 9105                      	lpm r16, Z+
00009e 930d                      	st X+, r16
00009f 5051                      	subi ITCOUNTERL, 1
0000a0 4040                      	sbci ITCOUNTERH, 0
0000a1 f7d8                      	brcc FillLoop
                                 
                                 
                                 	; *** PARSING STAGE ***
                                 
                                 	; Now, we will separate the numbers into divisible by 5 and 
                                 	; not divisible by 5
                                 
                                 	; Initialize again the Iteration Counter with 299
0000a2 e25b                      	ldi ITCOUNTERL, (LASTITERATION & 0xFF)
0000a3 e041                      	ldi ITCOUNTERH, ((LASTITERATION >> 8) & 0xFF)
                                 	; X will point to the start of STARTADDS
0000a4 e0b2                      	ldi XH, high(STARTADDS)
0000a5 e2a2                      	ldi XL, low(STARTADDS)
                                 	; Y will point to the start of DIVBY5
0000a6 e0d4                      	ldi YH, high(DIVBY5)
0000a7 e0c0                      	ldi YL, low(DIVBY5)
                                 	; Z will point to the start of NOTDIVBY5
0000a8 e0f6                      	ldi ZH, high(NOTDIVBY5)
0000a9 e0e0                      	ldi ZL, low(NOTDIVBY5)
                                 	; Initilize counters of multiples and not multiples of 5
0000aa 2766                      	clr DIVCOUNTERH
0000ab 2777                      	clr DIVCOUNTERL
0000ac 2788                      	clr UNDIVCOUNTERH
0000ad 2799                      	clr UNDIVCOUNTERL
0000ae 2733                      	clr r19
0000af e021                      	ldi r18,1
                                 
                                 DivideLoop:
0000b0 910d                      	ld r16, X+
0000b1 2f10                      	mov r17, r16	; Save it for later
                                 	; We will check divisibility using successive subtrations of
                                 	; 5 from r16 until the carry flag is set
                                 DivideLoopAgain:
0000b2 5005                      	subi r16, 5
0000b3 f7f0                      	brcc DivideLoopAgain
0000b4 5f0b                      	subi r16, -5	; This the same as adding 5
0000b5 3000                      	cpi r16,0
0000b6 f021                      	breq RemainingIsZero
0000b7 9311                      		st Z+, r17
0000b8 0f92                      		add UNDIVCOUNTERL, r18
0000b9 1f83                      		adc UNDIVCOUNTERH, r19
0000ba c003                      		rjmp DivideCheckEnd
                                 RemainingIsZero:
0000bb 9319                      		st Y+, r17
0000bc 0f72                      		add DIVCOUNTERL, r18
0000bd 1f63                      		adc DIVCOUNTERH, r19
                                 
                                 	; Now, we will check if the 300 iterations limit was reached.
                                 DivideCheckEnd:
0000be 5051                      	subi ITCOUNTERL, 1
0000bf 4040                      	sbci ITCOUNTERH, 0
0000c0 f778                      	brcc DivideLoop
                                 
                                 	
                                 	; **** ADDITIONS STAGE ***
                                 
                                 	; Initialize
                                 	; X will point to the start of DIVBY5
0000c1 e0b4                      	ldi XH, high(DIVBY5)
0000c2 e0a0                      	ldi XL, low(DIVBY5)
                                 	; Y will point to the start of NOTDIVBY5
0000c3 e0d6                      	ldi YH, high(NOTDIVBY5)
0000c4 e0c0                      	ldi YL, low(NOTDIVBY5)
                                 
                                 	; Zero our sum registers r16:r17 and r18_r19
0000c5 2700                      	clr r16		
0000c6 2711                      	clr r17
0000c7 2722                      	clr r18
0000c8 2733                      	clr r19
0000c9 2400                      	clr r0		; This register will be used as a dummy 0 to perform addition with carry
0000ca e043                      	ldi r20, 3	; This register works as a flag to control the flow
                                 
                                 	; Our sums loop will start here
                                 SumLoop:
0000cb 3041                      	cpi r20, 1		; Have with done only additions of multiples of 5 in the last iteration?
0000cc f011                      	breq SumDivBy5
0000cd 3043                      	cpi r20, 3		; Also all kinds of additions in the last iteration?
0000ce f449                      	brne SumNotDiv	; Skip if we are done with the multiples of 5
                                 SumDivBy5:
0000cf 9488                      	clc
0000d0 5071                      	subi DIVCOUNTERL,1
0000d1 4060                      	sbci DIVCOUNTERH, 0
0000d2 7f4e                      	andi r20, 0xfe	; Clear up our flag - assume we are not adding multiples of 5
0000d3 f020                      	brcs SumNotDiv
0000d4 6041                      		ori r20,1	; We are indeed adding multiples of 5
0000d5 901d                      		ld r1, X+	; Load from memory to a temp register and increment memory pointer
0000d6 0d11                      		add r17, r1
0000d7 1d00                      		adc r16, r0
                                 
                                 SumNotDiv:
                                 	; Have with done additions of non-multiples of 5 in the last iteration but not addition of multiples of 5 in this itertation?
0000d8 3042                      	cpi r20, 2
0000d9 f011                      	breq SumNotDivBy5
                                 	; Have with done additions of non-multiples of 5 in the last iteration and addition of multiples of 5 in this itertation?
0000da 3043                      	cpi r20, 3
0000db f449                      	brne SumCheckEnd
                                 SumNotDivBy5:
0000dc 9488                      	clc
0000dd 5091                      	subi UNDIVCOUNTERL,1
0000de 4080                      	sbci UNDIVCOUNTERH, 0
0000df 7f4d                      	andi r20, 0xfd	; Clear up our flag for no-multiples of 5.
0000e0 f020                      	brcs SumCheckEnd
0000e1 6042                      		ori r20, 2
0000e2 9019                      		ld r1, Y+	; Load from memory to a temp register and increment memory pointer
0000e3 0d31                      		add r19, r1
0000e4 1d20                      		adc r18, r0
                                 
                                 SumCheckEnd:
0000e5 3040                      	cpi r20, 0	; If no operation has been done in this iteration we are done.
0000e6 f721                      	brne SumLoop
                                 
                                 ; Expected result	
                                 ; Sum multiles of 5 = 0x1ed7
                                 ; Sum not multiples of 5 = 0x7663
                                 
                                 stop:
0000e7 cfff                      	rjmp stop
                                 	
                                 	
                                 
